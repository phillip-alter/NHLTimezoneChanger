{
    "name": "NHL Timezone Changer",
    "description": "Changes the timezone on NHL.com",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "nhltimezone.html",
        "default_icon": "images/nhltz.png"
    },
      "content_scripts": [
        {
          "js": ["scripts/content.js"],
          "matches": ["https://www.nhl.com/*"]
        }
      ]
}